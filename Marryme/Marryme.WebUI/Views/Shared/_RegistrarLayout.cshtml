<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    <link href="~/Content/bootstrap 4/css/MENTOR2style.css" rel="stylesheet" />
    <link href="~/assets/vendor/toastr/toastr.css" rel="stylesheet" />
    <link href="~/assets/vendor/iziToast/iziToast-min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery/jquery.min.js"></script>
    <script src="~/Scripts/angular.js"></script>
    <script src="~/assets/vendor/toastr/toastr.js"></script>
    <script src="~/assets/vendor/iziToast/iziToast-min.js"></script>
    <script src="~/assets/vendor/parsleyjs/js/parsley.js"></script>
    <script src="~/assets/vendor/php-email-form/validate.js"></script>
    <script src="~/Angular/Generic.js"></script>
    <script src="~/assets/js/ui-bootstrap-tpls-0.13.4.min.js"></script>
    <script src="~/Scripts/ng-file-upload.min.js"></script>
    <script src="~/Angular/Method.js"></script>
    <script src="~/Angular/Filter.js"></script>
    <script src="~/Angular/Service.js"></script>
    <link href="~/assets/vendor/parsleyjs/css/parsley.css" rel="stylesheet" />
    <link href="~/assets/assets/Side-Bar/style.css" rel="stylesheet" />
    <link href="~/assets/vendor/icofont/icofont.min.css" rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.2/css/all.min.css" rel="stylesheet" />
    <script src="~/Scripts/peerjs.min.js"></script>
    @Scripts.Render("~/bundles/modernizr")
    <style>
        .dropdown:hover > .dropdown-menu {
            display: block;
        }
        .navbar-light .navbar-nav .nav-link {
            color: rgb(40 38 38);
        }
        /*.navbar-light .navbar-nav .nav-link {
            color: #f8f9fa;
        }

            .navbar-light .navbar-nav .nav-link:hover {
                color: #f8f9fa;
            }

            .navbar-light .navbar-nav .nav-link.active {
                color: #0a79e9;
            }*/

        .text-success {
            color: #28a745 !important;
            background-color: white;
            border-radius: 10px;
            padding: 5px 13px;
            box-shadow: 7px 4px 8px #1e7e46;
            box-shadow: 34px;
        }

        .form-group {
            margin-bottom: 0.3rem
        }

        .btn-sky {
            color: #17a2b8 !important;
            font-weight: 600;
            font-size: 13px;
            letter-spacing: 0.5px;
            text-transform: uppercase;
            display: inline-block;
            padding: 12px 30px;
            border-radius: 50px;
            transition: 0.3s;
            line-height: 1;
            color: white;
            border: 2px solid #17a2b8;
        }

        .btn-success {
            color: #f8f9fa;
            background-color: #28a745;
            border-color: #0062cc;
            border: 2px solid #879aab;
        }

        .card-header {
            padding: 0.3rem 1.25rem;
            margin-bottom: 0;
            background-color: rgba(0, 0, 0, 0.03);
            border-bottom: 1px solid rgba(0, 0, 0, 0.125);
        }

        .btn-sky:hover {
            background: #6ee388;
            color: #fff !important;
        }
    </style>
</head>
<body>
    <div>
        <div ng-app="MarrymeClientModule" class="wrapper align-items-stretch">
            @if (Session["MemberId"] != null)
            {
                <header class="fixed-top">
                    <div class="navbar navbar-expand-md navbar-light bg-white ">

                        <a class="navbar-brand text-success" href="#" style=" font-size: xx-large; font-weight: bolder; padding-left: 5px;">1234wife</a>
                        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarContent">
                            <span class="navbar-toggler-icon"></span>
                        </button>
                        <div class="collapse navbar-collapse text-center" id="navbarContent">
                            <ul class="navbar-nav ml-auto pull-right" style="font-weight: 700;">
                                <li class="nav-item">
                                    <a class="nav-link active" onclick="green()" id="Green" href="@Url.Action("Index","Home")"><i class="fa fa-home mr-3"></i>Home</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link active" onclick="green1()" id="Green9" href="@Url.Action("ProfileWizard","Member")"><i class="fa fa-user mr-3"></i>My Profile</a>
                                </li>
                                @*<li class="nav-item">
                                        <a class="nav-link active" onclick="green1()" id="Green1" href="@Url.Action("MemberList","Member")"><i class="fa fa-list mr-3"></i>Member List</a>
                                    </li>*@
                                <li class="nav-item">
                                    <a class="nav-link active" onclick="green1()" id="Green10" href="#"><i class="fa fa-info-circle mr-3"></i>About</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link active" onclick="green7()" id="Green7" href="#l"><i class="fa fa-contact-card mr-3"></i>Contact</a>
                                </li>
                                <li>
                                    <a href="@Url.Action("Logout","Account")" type="button" class="btn btn-success" style="border-radius: 30px; width: 130px;"><i class="fa fa-sign-out mr-3"></i>Log Out</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </header>
                <!--Nav Bar End-->
                <div class=" body-content">
                    @RenderBody()
                    <hr />
                    <footer>
                        <p class="text-center">&copy; @DateTime.Now.Year - 1234Wife.com</p>
                    </footer>
                    <div id="preloader"></div>
                    @if (Session["MemberId"] != null && !Request.Url.AbsolutePath.ToLower().Contains("/memberchat/livechat"))
                    {
                        <a href="@Url.Action("LiveChat","MemberChat")" class="back-to-top chatbtn">
                            <span id="chatCounter" class="badge text-success position-absolute" style="right:50px;bottom:50px;">0</span>
                            <i class="far fa-comments text-success" style="font-size:50px"></i>
                        </a>
                        <script>
                            Get('/MemberChat/UnReadCounterByMemberId').then(function (result) {
                                //    console.log(result);
                                $('#chatCounter').html(result.Data);
                            });
                        </script>
                    }
                </div>
            }
            else
            {
                <!--Nav Bar Start-->
                <header class="fixed-top" style="display:inline-block">
                    <div class="navbar navbar-expand-lg navbar-light bg-info text-white ">

                        <a class="navbar-brand text-success" href="#" style="font-size: xx-large; font-weight: bolder; padding-left:5px;">1234wife</a>
                        @*<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarContent">
                                <span class="navbar-toggler-icon"></span>
                            </button>*@
                        <div class="collapse navbar-collapse text-center" id="navbarContent">
                            <ul class="navbar-nav ml-auto pull-right" style="font-weight: 700;">
                                <li class="nav-item">
                                    <a class="nav-link active" onclick="green()" id="Green" href="@Url.Action("Index","Home")"><span class="fa fa-home mr-3"></span>Home</a>
                                </li>
                                <li class="nav-item">
                                    @*<a class="nav-link active" onclick="green4()" id="Green4" href="#">Events</a>*@
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link active" onclick="green1()" id="Green1" href="#"><i class="fa fa-circle-info mr-3"></i>About</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link active" onclick="green7()" id="Green7" href="#l"><i class="fa fa-contact-card mr-3"></i>Contact</a>
                                </li>
                                <li class="nav-item dropdown">
                                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                        <i class="fa fa-registered mr-3"></i> Register
                                    </a>
                                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                        <a class="nav-link active" onclick="green8()" id="Green8"
                                           href="/Account/Registeration/">
                                            Member Register
                                        </a>
                                        <div class="dropdown-divider"></div>
                                        <a class="nav-link active" onclick="green8()" id="Green9"
                                           href="/Account/RegisterRegistrar/">
                                            Registrar Register
                                        </a>
                                    </div>
                                </li>
                                @*<li class="nav-item">
                                        <a class="nav-link active" onclick="green8()" id="Green8" href="/Account/Registeration/"><i class="fa fa-registered mr-3"></i>Register</a>
                                    </li>*@
                                <a href="@Url.Action("Login","Account")" type="button" class="btn btn-success" style="border-radius: 30px; width: 130px; height:40px"><i class="fa fa-sign-in mr-3"></i>Login</a>
                            </ul>
                        </div>
                    </div>
                </header>
                <!--Nav Bar End-->
                <div class=" body-content">
                    @RenderBody()
                    <hr />
                    <footer>
                        <p>&copy; @DateTime.Now.Year - 1234Wife.com</p>
                    </footer>

                </div>
            }


            @*@Scripts.Render("~/bundles/jquery")*@
            @Scripts.Render("~/bundles/bootstrap")
            @RenderSection("scripts", required: false)


            <div class="modal fade" id="loginModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header bg-success">
                            <h5 class="modal-title text-white" id="exampleModalLabel">Login</h5>
                            <button class="close text-white" type="button" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">×</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            @Html.Partial("~/Views/Partials/_Login.cshtml")
                        </div>
                        <div class="modal-footer bg-success">
                            <button class="btn btn-outline-light" type="button" data-dismiss="modal">Cancel</button>
                            <br />
                        </div>
                    </div>
                </div>
            </div>
            @*@Html.Partial("~/Views/Partials/_ChangePassword.cshtml")*@
        </div>
    </div>
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
