                                        </div>
                                        <div class="col-sm-3">
                                            <img src="~/Images/MemberDetail/dont_drink.jpg" /><p>{{MemberDetail.DrinkingHabit||"N/A"}}</p>
                                        </div>*@
                                    <div class="col-sm-3">
                                        <img src="~/Images/MemberDetail/no_smoking.jpg" /><p>{{MemberDetail.SmokingHabit||"N/A"}}</p>
                                    </div>
                                    <div class="col-sm-3">
                                        <img src="~/Images/MemberDetail/complexion-bulid.jpg" /><p>{{MemberDetail.Complexion||"N/A"}}</p>
                                    </div>
                                </div>
                            </div>
                        </div><br />
