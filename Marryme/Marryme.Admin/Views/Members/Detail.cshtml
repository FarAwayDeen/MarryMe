                        <!-- Accordion card -->
                        <div class="card">

                            <!-- Card header -->
                            <div class="card-header bg-white">
                                BACKGROUND
                            </div>

                            <div class="card-body">
                                <div class="profile_icons">
                                    <div class="col-sm-3">
                                        <img src="~/Images/MemberDetail/religion.jpg" /><p>{{MemberDetail.Religion||"N/A"}}</p>
                                    </div>
                                    <div class="col-sm-3">
                                        <img src="~/Images/MemberDetail/religion (1).jpg" /><p>{{MemberDetail.Language||"N/A"}}</p>
                                    </div>
                                    <div class="col-sm-3">
                                        <img src="~/Images/MemberDetail/live_location.jpg" /><p>{{MemberDetail.State||"N/A"}}</p>
                                    </div>
                                    <div class="col-sm-3">
                                        <img src="~/Images/MemberDetail/Grew_up.jpg" /><p>{{MemberDetail.Country||"N/A"}}</p>
                                    </div>
                                </div>
                            </div>

                        </div><br />
                        <!-- Accordion card -->
                        <!-- Accordion card -->
                        <div class="card">

                            <!-- Card header -->
                            <div class="card-header bg-white">
                                EDUCATION & CAREER
                            </div>

                            <div class="card-body">
                                <div class="profile_icons">
                                    <div class="col-sm-3">
                                        <img src="~/Images/MemberDetail/master_degree.jpg" /><p>{{MemberDetail.HighestEducation||"N/A"}}</p>
                                    </div>
                                    <div class="col-sm-3">
                                        <img src="~/Images/MemberDetail/team_member.jpg" /><p>{{MemberDetail.Occupation||"N/A"}}</p>
