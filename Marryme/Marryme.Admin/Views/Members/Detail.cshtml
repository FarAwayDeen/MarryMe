                                    <div class="col-sm-3">
                                        <img src="~/Images/MemberDetail/earn.jpg" /><p>{{MemberDetail.AnualIncome||"N/A"}}</p>
                                    </div>
                                </div>
                            </div>
                        </div><br />
                        <!-- Accordion card -->
                        <!-- Accordion card -->
                        <div class="card">

                            <!-- Card header -->
                            <div class="card-header">
                                <p>
                                    MORE DETAILS
                                </p>
                            </div>

                            <div class="card card-body">
                                <div class="container">
                                    <div class="row" style="margin-top:10px">
                                        <div class="col-sm-3">
                                            <div class="card">
                                                <div class="card-body">
                                                    <P>Family Status:{{MemberDetail.FamilyStatus||"N/A"}}</P>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-3">
                                            <div class="card">
                                                <div class="card-body">
                                                    <P>Father Alive:{{MemberDetail.IsFatherAlive||"N/A"}}</P>
                                                </div>
                                            </div>
                                        </div>


                                    </div>
                                    <div class="row">
                                        <div class="col-sm-3">
                                            <div class="card">
                                                <div class="card-body">
                                                    <P>Mother Alive:{{MemberDetail.IsMotherAlive||"N/A"}}</P>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-3">
                                            <div class="card">
                                                <div class="card-body">
                                                    <P>No.of Brothers:{{MemberDetail.NoOfBrothers||"N/A"}}</P>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                    <div class="row">
                                        <div class="col-sm-3" style="margin-top:10px">
                                            <div class="card">
                                                <div class="card-body">
                                                    <P>No.of Sisters:{{MemberDetail.NoOfSisters||"N/A"}}</P>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div><br />
                        <!-- Accordion card -->
                    </div>
                    <!-- Accordion wrapper -->
                </div>
            </div>
        </div>
