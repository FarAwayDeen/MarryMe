
@{
    ViewBag.Title = "MemberDetail";
}
<script src="~/Angular/Members/MemberDetail.js?v=@DateTime.Now.Ticks"></script>
@*<link href="~/Content/UserDetail.css" rel="stylesheet" />*@
<div class="bg-white" ng-controller="MemberDetailController">
    <div class="container">
        <div class="row">
            <div class="col-sm-3" style="font-size:14px">
                <p>
                    Membership type:&nbsp;<span>3 Star</span>
                    <h4>{{MemberDetail.Name}} (EF00{{MemberDetail.Id}})</h4>
                    profile created by | <strong>{{MemberDetail.ProfileCreatedBy}}</strong>
                </p>
            </div>
        </div>
        <div class="row">
            @*<div class="col-sm-3 col-12">
                    <div class="row">
                        <div class="col-7">
                            <a href="#">
                                <img src="~/Images/2.jpg" class="rounded" style="height:200px;width:200px" />
                            </a>
                        </div>
                        <div class="col-6 text-center">
                            <a href="#" class="btn btn-sm" id="registerbtn">Click to like</a>
                        </div>
                    </div>
                </div>*@
            <div class="col-sm-8">
                <div class="card form-group">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-sm-9">
                                <div class="card-text">
                                    <p><strong>Name:&nbsp;</strong>{{MemberDetail.Name}}</p>
                                    <p><strong>DOB:&nbsp;</strong>{{MemberDetail.DOB|jsonDate|date: MM-dd-yyyy}}</p>
                                    <p><strong>Religion:&nbsp;</strong>{{MemberDetail.Religion}}</p>
                                    <p><strong>Marital Status:&nbsp;</strong>{{MemberDetail.MaritalStatus}}</p>
                                    <p><strong>Location:&nbsp;</strong>{{MemberDetail.Country}}</p>
                                    <p><strong>Cast:&nbsp;</strong>{{MemberDetail.Subcast}}</p>
                                    <p><strong>LifeStyle:&nbsp;</strong>Will tel you later</p>
                                    <p><strong>Culture:&nbsp;</strong>Punjabi</p>
                                    <p><strong>Educatoin:&nbsp;</strong>{{MemberDetail.HighestEducation}}</p>
                                    <p><strong>Occupation:&nbsp;</strong>{{MemberDetail.Occupation}}</p>
                                    <p><strong>Contact:&nbsp;</strong>{{MemberDetail.MobileNo}}</p>
                                    <p><strong>Language:&nbsp;</strong>{{MemberDetail.Language}}</p>
                                </div>
                            </div>
                            <div class="col-sm-3">
                                <div class="col-sm-8">
                                    <a href="#">
                                        <img src="{{MemberDetail.ProfilePicPath}}" class="rounded" style="width:180px;margin-top:-10px;margin-left:-36px" onerror="CheckImage(this)" />
                                    </a>
                                </div>
                                @*<div class="col-4">
                                        <a href="#" class="btn btn-sm" id="registerbtn" style="width:115px;margin-top:1px">Click to like</a>
                                    </div>*@
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <!--Accordion wrapper-->
                    <div class="">

                        <!-- Accordion card -->
                        <div class="card">

                            <!-- Card header -->
                            <div class="card-header bg-white">
                                <p>

                                    ABOUT MY SELF
                                </p>
                            </div>

                            <div class="card-body">
                                <P>
                                    {{MemberDetail.AboutMe||"N/A"}}
                                </P>
                            </div>

                        </div><br />
                        <!-- Accordion card -->
                        <!-- Accordion card -->
                        <div class="card">

                            <!-- Card header -->
                            <div class="card-header bg-white">
                                LIFE STYLE & APPEARANCE
                            </div>

